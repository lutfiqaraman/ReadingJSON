@model IEnumerable<StockOverflow.Models.Item>


@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<ul>
    @foreach (var item in Model)
    {
        <li>
            <h4>@item.title</h4>

            @foreach (var tag in item.tags)
            {
                <p>@tag</p>
            }
        </li>
    }
</ul>
